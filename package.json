{
  "name": "react-native-embryo",
  "version": "0.1.0+1",
  "description": "The unborn child of react native apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/aino/react-native-embryo"
  },
  "author": "Aino",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/aino/react-native-embryo/issues"
  },
  "scripts": {
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "android:release": "./bin/android-release",
    "android:staging": "./bin/android-staging",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && cd android && ./gradlew clean && cd ..",
    "clean:wipe": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf node_modules/* && yarn cache clean && yarn install --check-files && cd android && ./gradlew clean && cd ..",
    "flow": "flow-scripts stub && flow",
    "flow:stop": "flow stop",
    "ios": "react-native run-ios --scheme Debug",
    "ios:release": "ENVFILE=.env.production react-native run-ios --scheme Release --configuration Release",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start": "./bin/init",
    "v": "./bin/version"
  },
  "pre-commit": [
    "precommit-msg"
  ],
  "dependencies": {
    "core-decorators": "^0.20.0",
    "json": "^9.0.6",
    "lodash.isequal": "^4.5.0",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0-alpha.1",
    "react-native": "0.54.2",
    "react-native-config": "^0.11.5",
    "react-native-navigation": "^1.1.409",
    "react-native-restart": "^0.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.68.0",
    "flow-scripts": "^0.4.0",
    "flow-typed": "^2.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "solidarity": "^2.0.2"
  }
}
